{
  "manifest_version": 2,
  "name": "Save link to Bookmarks.dev",
  "version": "2.2.0",
  "description": "Save the active tab's URL Bookmarks.dev",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "browser-polyfill.0.6.0.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon128.png"
  },
  "homepage_url": "https://www.bookmarks.dev"
}
